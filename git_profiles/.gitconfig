[alias]
	lg = log --oneline --decorate --graph
	co = checkout
	cob = checkout -b
	comp = "!f() { git rev-list --left-right --count $branch...$2; }; f"
	s = status
	b = branch
    col = "!f() { git branch; }; f"
    unstage = reset HEAD --
